{
	"name": "@ctx-core/jwt",
	"version": "10.0.30",
	"description": "ctx-core jwt",
	"keywords": [
		"ctx-core",
		"jwt"
	],
	"homepage": "https://github.com/ctx-core/jwt#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/jwt/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/jwt/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/atob": "workspace:^10.0.25",
		"@ctx-core/error": "workspace:^11.0.20",
		"@ctx-core/function": "workspace:^20.2.5",
		"@swc/cli": "^0.1.51",
		"@swc/core": "^1.2.110"
	},
	"devDependencies": {
		"typescript": "^4.5.2"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}