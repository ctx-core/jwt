{
	"name": "@ctx-core/jwt",
	"version": "10.0.41",
	"description": "ctx-core jwt",
	"keywords": [
		"ctx-core",
		"jwt"
	],
	"homepage": "https://github.com/ctx-core/jwt#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/jwt/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/jwt.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./src/index.ts"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/atob": "workspace:^10.0.35",
		"@ctx-core/error": "workspace:^11.1.12",
		"@ctx-core/function": "workspace:^20.2.15",
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120"
	},
	"devDependencies": {
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./dist/index.js"
}