{
	"name": "@ctx-core/jwt",
	"version": "10.5.27",
	"description": "ctx-core jwt",
	"keywords": [
		"ctx-core",
		"jwt"
	],
	"homepage": "https://github.com/ctx-core/jwt#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/jwt/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/jwt.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./_types",
		"./authorization__header__jwt_token_",
		"./jwt__expiration__error_",
		"./jwt__expiration__is_valid_",
		"./jwt__expiration__validate",
		"./jwt_token_exp_"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/atob": "^10.2.27",
		"@ctx-core/error": "^12.4.26",
		"@ctx-core/function": "^21.22.22"
	},
	"devDependencies": {
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.5.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}