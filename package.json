{
	"name": "@ctx-core/jwt",
	"version": "10.0.46",
	"description": "ctx-core jwt",
	"keywords": [
		"ctx-core",
		"jwt"
	],
	"homepage": "https://github.com/ctx-core/jwt#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/jwt/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/jwt.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist && npm run clean:tsbuildinfo",
		"clean:tsbuildinfo": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.d.ts",
		"compile": "npm run compile:source && npm run compile:declaration",
		"compile:source": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"compile:declaration": "npm run clean:tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir dist",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/atob": "workspace:^10.0.40",
		"@ctx-core/error": "workspace:^11.1.17",
		"@ctx-core/function": "workspace:^20.2.20",
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.123"
	},
	"devDependencies": {
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./dist/index.js"
}